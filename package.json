{
	"name": "backend",
	"packageManager": "yarn@3.4.1",
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"start": "node dist/index.js",
		"dev": "nodemon",
		"migration:create": "yarn build && ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create --dataSource ormconfig-migration.js ",
		"migration:generate": "yarn build && ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:generate --dataSource ormconfig-migration.js ",
		"migration:run": "yarn build && ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:run --dataSource ormconfig-migration.js"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.293.0",
		"@aws-sdk/lib-storage": "^3.294.0",
		"@fastify/cors": "^8.2.0",
		"@fastify/multipart": "^7.5.0",
		"@fastify/rate-limit": "^8.0.0",
		"@fastify/static": "^6.9.0",
		"async-injection": "^1.5.2",
		"axios": "^1.3.4",
		"dotenv": "^16.0.3",
		"fastify": "^4.13.0",
		"fastify-multer": "^2.0.3",
		"fastify-plugin": "^4.5.0",
		"fastify-raw-body": "^4.2.0",
		"file-type": "^18.2.1",
		"ioredis": "^5.3.1",
		"node-email-check": "^1.0.4",
		"pg": "^8.9.0",
		"pino": "^8.11.0",
		"pwd-strength": "^1.1.2",
		"reflect-metadata": "^0.1.13",
		"sib-api-v3-typescript": "^2.2.2",
		"stripe": "^11.14.0",
		"typeorm": "0.3.11",
		"validate-image-type": "^2.0.0"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20230307.0",
		"@types/node": "^18.14.5",
		"@types/pwd-strength": "^1.1.0",
		"@types/request": "^2.48.8",
		"nodemon": "^2.0.21",
		"pino-pretty": "^9.4.0",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^3.14.2",
		"typescript": "^4.9.5",
		"wrangler": "^2.12.3"
	}
}
